<Window x:Class="GameSSSS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameSSSS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas x:Name="Canvas1" KeyDown="KeyIsDown" KeyUp="KeyIsUp" Focusable="True">
        <Ellipse x:Name="player" Width="30" Height="30" Fill="Red" Canvas.Left="10" Canvas.Top="10" ></Ellipse>
    </Canvas>

</Window>
